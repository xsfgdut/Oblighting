<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dky="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.ob.obsmarthouse.common.widget.TopTitle
        android:id="@+id/position_frag_tt"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mid_content_h"
        dky:title_text="@string/position_room"
        dky:right_text_src="@string/creat"
        dky:right_text_size="@dimen/lit_textsize"
        dky:title_text_size="@dimen/mid_textsize"
         />

    <ListView
        android:id="@+id/position_frag_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/position_frag_tt" />
</RelativeLayout>